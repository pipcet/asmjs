//** ThinThin.sbrk (unused)
ThinThin.sbrk = function (size)
{
    size = (size + 4095) & -4096;
    var ret = this.%{zp[&zeropage::top_of_sbrk]};

    this.%{zp[&zeropage::top_of_sbrk]} += size;

    if (this.%{zp[&zeropage::top_of_sbrk]} > this.%{zp[&zeropage::top_of_memory]}) {
        throw "OOM";
    }

    return ret;
};
