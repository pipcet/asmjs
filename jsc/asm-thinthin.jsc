    port.onmessage = event => {
        var data = event.data;
        var kind = data[0];

        if (kind === "write") {
            this.provideString(data[1]);
        } else if (kind === "read") {
            n = data[1];

            handler();
        }
    };

    port.postMessage(["read"]);
};

