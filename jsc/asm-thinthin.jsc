//*** sendflow (unused?)
function sendflow(flow1, flow2, n, count)
{
    if (count === undefined)
        count = 0;

    if (n === undefined)
        n = Infinity;

    if (n === 0)
        return Promise.resolve(count);

    flow1.read(n).then(s => {
        return flow2.write(s);
    }).then(k => {
        count += k;
        n -= k;

        return sendflow(flow1, flow2, n, count);
    });
}
