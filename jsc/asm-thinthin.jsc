            this.readEOF) {
            if (n > this.readData.length - this.readPosition)
                n = this.readData.length - this.readPosition;
            port.postMessage(this.readData.substr(this.readPosition, n));
            this.consume(n);
        }

        this.onstuff.add(handler);
