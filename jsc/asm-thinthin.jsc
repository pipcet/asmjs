    ThinThin.ioctl_p = function (fdno, code, intptr) {
        fdno = 0;
        var fd = this.fds[fdno];

        switch (code) {
        case %{FIONREAD}:
            return fd.available().then(avail => {
                if (avail) {
                    %{*intptr = "avail"}
                } else {
